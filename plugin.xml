      <extension id="org.eclipse.ui.idexportwizard" name="Export Wizard Extension Point"
            point="org.eclipse.ui.exportWizards">
            <wizard name="ExportDDIXml" icon="icons/properties.gif"
                  class="org.ddialliance.ddieditor.ui.wizard.export.ExportDdiXmlWizard"
                  id="org.ddialliance.ddieditor.ui.wizard.exportddi.ExportDdiXmlWizard">
                  <description> %Export DDI </description>
            </wizard>
      </extension>
      <extension id="product" point="org.eclipse.core.runtime.products">
            <product application="DDIEditor.application" name="DDI Editor">
                  <property name="appName" value="DDI Editor"> </property>
                  <property name="startupForegroundColor" value="000000"> </property>
                  <property name="startupMessageRect" value="7,252,445,20"> </property>
                  <property name="startupProgressRect" value="5,275,445,15"> </property>
                  <property name="preferenceCustomization" value="plugin_customization.ini"> </property>
                  <property name="aboutText"
                        value="DDI Editor (Beta Version)&#x0A;&#x0A;DDI Editor UI Version: 1.0.0&#x0A;DDI Manager Version: 1.0.0"> </property>
                  <property name="aboutImage" value="icons/alt_about.gif"> </property>
            </product>
      </extension>
      <extension point="org.eclipse.ui.preferencePages">
            <page class="org.ddialliance.ddieditor.ui.preference.page.DdiPreferencePage"
                  id="org.ddialliance.ddieditor.ui.preference.page.DdiPreferencePage" name="DDI"> </page>
            <page
                  class="org.ddialliance.ddieditor.ui.preference.page.DdiEditorBackendPreferencePage"
                  id="org.ddialliance.ddieditor.ui.preference.page.DdiEditorBackendPreferencePage"
                  name="DDIEditor Backend"> </page>
            <page class="org.ddialliance.ddieditor.ui.preference.page.DdiEditorUiPreferencePage"
                  id="org.ddialliance.ddieditor.ui.preference.page.DdiEditorUiPreferencePage"
                  name="DDIEditor UI"> </page>
      </extension>
      <extension point="org.eclipse.core.runtime.preferences">
            <initializer class="org.ddialliance.ddieditor.ui.preference.PreferenceInitializer"
            > </initializer>
      </extension>
      <extension point="org.eclipse.ui.startup">
            <startup class="org.ddialliance.ddieditor.ui.perspective.AutoChangePerspectiveListener"
            />
      </extension>
</plugin>
