<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
      <extension-point id="org.ddialliance.ddieditor.extensionpoints.addattr"
            name="AdditionalAttributes"
            schema="schema/org.ddialliance.ddieditor.extensionpoints.addattr.exsd"/>

      <extension id="application" point="org.eclipse.core.runtime.applications">
            <application>
                  <run class="org.ddialliance.ddieditor.ui.Application"> </run>
            </application>
      </extension>
      <extension point="org.eclipse.ui.perspectives">
            <perspective class="org.ddialliance.ddieditor.ui.perspective.ConceptsPerspective"
                  id="org.ddialliance.ddieditor.ui.perspective.ConceptsPerspective" name="Concepts"> </perspective>
            <perspective class="org.ddialliance.ddieditor.ui.perspective.CodesPerspective"
                  id="org.ddialliance.ddieditor.ui.perspective.CodesPerspective" name="Codes"> </perspective>
            <perspective class="org.ddialliance.ddieditor.ui.perspective.InfoPerspective"
                  id="org.ddialliance.ddieditor.ui.perspective.InfoPerspective" name="Overview"> </perspective>
            <perspective name="Questions"
                  class="org.ddialliance.ddieditor.ui.perspective.QuestionsPerspective"
                  id="org.ddialliance.ddieditor.ui.perspective.QuestionsPerspective"> </perspective>
            <perspective name="Instrument"
                  class="org.ddialliance.ddieditor.ui.perspective.InstrumentPerspective"
                  id="org.ddialliance.ddieditor.ui.perspective.InstrumentPerspective"
            > </perspective>
      </extension>
      <extension point="org.eclipse.ui.views">
            <view class="org.ddialliance.ddieditor.ui.view.InfoView" icon="icons/tree.gif"
                  id="org.ddialliance.ddieditor.ui.view.InfoView" name="OverView"> </view>
            <view class="org.ddialliance.ddieditor.ui.view.ConceptView" icon="icons/tree.gif"
                  id="org.ddialliance.ddieditor.ui.view.ConceptView" name="Concepts"> </view>
            <view class="org.ddialliance.ddieditor.ui.view.CodeView" icon="icons/tree.gif"
                  id="org.ddialliance.ddieditor.ui.view.CodeView" name="Codes" restorable="true"> </view>
            <view class="org.ddialliance.ddieditor.ui.view.QuestionItemView" icon="icons/tree.gif"
                  id="org.ddialliance.ddieditor.ui.view.QuestionItemView" name="Questions"> </view>
            <view class="org.ddialliance.ddieditor.ui.view.InstrumentView" icon="icons/tree.gif"
                  id="org.ddialliance.ddieditor.ui.view.InstrumentView" name="Instrument"
                  restorable="true"> </view>
      </extension>
      <extension point="org.eclipse.ui.menus">
            <menuContribution locationURI="org.ddialliance.ddieditor.ui.menuContribution1"
            > </menuContribution>
      </extension>
      <extension point="org.eclipse.ui.editors">
            <editor class="org.ddialliance.ddieditor.ui.editor.question.QuestionSchemeEditor"
                  default="false" icon="icons/editor_area.gif"
                  id="org.ddialliance.ddieditor.ui.editor.question.QuestionSchemeEditor"
                  name="QuestionSchemeEditor"> </editor>
            <editor class="org.ddialliance.ddieditor.ui.editor.question.QuestionItemEditor"
                  default="false" icon="icons/editor_area.gif"
                  id="org.ddialliance.ddieditor.ui.editor.question.QuestionItemEditor"
                  name="QuestionItemEditor"> </editor>
            <editor class="org.ddialliance.ddieditor.ui.editor.concept.ConceptEditor"
                  default="false" icon="icons/editor_area.gif"
                  id="org.ddialliance.ddieditor.ui.editor.concept.ConceptEditor"
                  name="ConceptEditor"> </editor>
            <editor class="org.ddialliance.ddieditor.ui.editor.concept.ConceptSchemeEditor"
                  default="false" icon="icons/editor_area.gif"
                  id="org.ddialliance.ddieditor.ui.editor.concept.ConceptSchemeEditor"
                  name="ConceptSchemeEditor"> </editor>
            <editor class="org.ddialliance.ddieditor.ui.editor.code.CodeSchemeEditor"
                  default="false" icon="icons/editor_area.gif"
                  id="org.ddialliance.ddieditor.ui.editor.code.CodeSchemeEditor"
                  name="CodeSchemeEditor"> </editor>
            <editor class="org.ddialliance.ddieditor.ui.editor.study.StudyUnitEditor"
                  default="false" icon="icons/editor_area.gif"
                  id="org.ddialliance.ddieditor.ui.editor.study.StudyUnitEditor"
                  name="StudyUnitEditor"> </editor>
            <editor
                  class="org.ddialliance.ddieditor.ui.editor.instrument.InstrumentEditor"
                  default="false"
                  icon="icons/editor_area.gif"
                  id="org.ddialliance.ddieditor.ui.editor.instrument.InstrumentEditor"
                  name="InstrumentEditor">
            </editor>
            <editor
                  class="org.ddialliance.ddieditor.ui.editor.instrument.StatementItemEditor"
                  default="false"
                  icon="icons/editor_area.gif"
                  id="org.ddialliance.ddieditor.ui.editor.instrument.StatementItemEditor"
                  name="StatementItemEditor">
            </editor>
            <editor
                  class="org.ddialliance.ddieditor.ui.editor.instrument.IfThenElseEditor"
                  default="false"
                  id="org.ddialliance.ddieditor.ui.editor.instrument.IfThenElseEditor"
                  name="IfThenElseEditor">
            </editor>
      </extension>
