        <l:Code isDiscrete="true">
          <l:CategoryReference>
            <r:ID>Cat_11</r:ID>
          </l:CategoryReference>
          <l:Value>4</l:Value>
        </l:Code>
        <l:Code isDiscrete="true">
          <l:CategoryReference>
            <r:ID>Cat_12</r:ID>
          </l:CategoryReference>
          <l:Value>5</l:Value>
        </l:Code>
