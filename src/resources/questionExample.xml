        </l:Category>
        <l:Category id="Cat_8">
          <r:Label>Both parents (with or without other non-parents)</r:Label>
        </l:Category>
        <l:Category id="Cat_9">
          <r:Label>One parent (with or without other non-parents)</r:Label>
        </l:Category>
        <l:Category id="Cat_10">
          <r:Label>Non-parent relative (with or without other non-parents)</r:Label>
        </l:Category>
        <l:Category id="Cat_11">
          <r:Label>Non-relatives only</r:Label>
        </l:Category>
        <l:Category id="Cat_12">
          <r:Label>Live alone</r:Label>
        </l:Category>
      </l:CategoryScheme>
      <l:CodeScheme id="CODS_1">
        <l:CategorySchemeReference>
          <r:ID>CATS_1</r:ID>
        </l:CategorySchemeReference>
        <l:Code isDiscrete="true">
          <l:CategoryReference>
            <r:ID>Cat_3</r:ID>
          </l:CategoryReference>
          <l:Value>0</l:Value>
        </l:Code>
        <l:Code isDiscrete="true">
          <l:CategoryReference>
            <r:ID>Cat_4</r:ID>
          </l:CategoryReference>
          <l:Value>1</l:Value>
        </l:Code>
      </l:CodeScheme>
      <l:CodeScheme id="CODS_2">
        <l:CategorySchemeReference>
          <r:ID>CATS_1</r:ID>
        </l:CategorySchemeReference>
        <l:Code isDiscrete="true">
          <l:CategoryReference>
            <r:ID>Cat_1</r:ID>
          </l:CategoryReference>
          <l:Value>1</l:Value>
        </l:Code>
        <l:Code isDiscrete="true">
