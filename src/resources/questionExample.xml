        </l:Code>
      </l:CodeScheme>
      <l:CodeScheme id="CODS_3">
        <l:CategorySchemeReference>
          <r:ID>CATS_1</r:ID>
        </l:CategorySchemeReference>
        <l:Code isDiscrete="true">
          <l:CategoryReference>
            <r:ID>Cat_8</r:ID>
          </l:CategoryReference>
          <l:Value>1</l:Value>
        </l:Code>
        <l:Code isDiscrete="true">
          <l:CategoryReference>
            <r:ID>Cat_9</r:ID>
          </l:CategoryReference>
